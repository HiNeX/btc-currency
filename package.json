{
  "name": "btc-currency",
  "version": "1.0.3",
  "description": "BTC Currency checker",
  "main": "dist",
  "scripts": {
    "dev": "./node_modules/.bin/webpack -d && nodemon -w src --exec \"babel-node $NODE_DEBUG_OPTION src --presets es2015,stage-0\"",
    "build": "./node_modules/.bin/webpack -d && babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "airbnb"
    ],
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "HiNeX/coinboard-trade",
  "author": "Roman Hinex <roman@hinex.org>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.1",
    "compression": "^1.6.2",
    "express": "^4.14.1",
    "file": "^0.2.2",
    "file-loader": "^0.10.0",
    "mobx": "^3.1.0",
    "mobx-react": "^4.1.0",
    "moment": "^2.17.1",
    "morgan": "^1.8.1",
    "node-sass": "^4.5.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "request": "^2.79.0",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "webpack": "^2.2.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "nodemon": "^1.9.2",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.13.1"
  }
}
